    <mapping>
        <class-a>org.jgrades.lic.entities.LicencePropertyEntity</class-a>
        <class-b>org.jgrades.api.lic.model.LicenceProperty</class-b>
    </mapping>

    <mapping>
        <class-a>org.jgrades.lic.entities.ProductEntity</class-a>
        <class-b>org.jgrades.api.lic.model.Product</class-b>
    </mapping>
</mappings>
