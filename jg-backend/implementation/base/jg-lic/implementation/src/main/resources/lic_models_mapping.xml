        <class-b>org.jgrades.api.lic.model.Customer</class-b>
        <field>
            <a>customerId</a>
            <b>id</b>
        </field>
    </mapping>

    <mapping>
        <class-a>org.jgrades.lic.entities.LicenceEntity</class-a>
        <class-b>org.jgrades.api.lic.model.Licence</class-b>
